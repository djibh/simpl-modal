{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "es2015", "es2017"],
      "module": "commonjs",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "jsx": "react",
      "declaration": true,
      "outDir": "./dist",
      "strict": true
    },
    "include": ["src/**/*.ts", "src/**/*.tsx", "src/**/*.css", "src/**/*.svg"],
    "exclude": ["node_modules", "dist"]
  }
  